  },
  "keywords": [
    "css",
    "sass",
    "import",
    "webpack"
  ],
  "dependencies": {
    "async": "^3.2.2",
    "chalk": "^1.1.3",
    "glob": "^7.1.4",
    "loader-utils": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "eslint": "^6.0.1",
    "jest": "^24.8.0",
    "raw-loader": "^0.5.1",
